					<div id="left">
						<div id="book-front">
							<img src="/uploads/<%= ideas.img %>" />	
						</div>
					</div>
					<div id="right">
						<h2 class="heading"><%=  ideas.title %></h2>
						<h4 class="heading"><%=  ideas.category %></h4>
						<p class="pera"><%=  ideas.description %></p>
						<a class="btn" href="/<%=  ideas.sellPrice  %>/payment" role="button"
						 id="download-btn">
						 Buy  Rs.<% if (ideas.sellPrice != null){ %>
							<%=  ideas.sellPrice  %> 
						  <% } else{ %>  
							0
						 <% } %>
						  </a>
